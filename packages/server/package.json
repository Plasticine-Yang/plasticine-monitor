{
  "name": "@plasticine-monitor/server",
  "private": true,
  "version": "0.0.2",
  "description": "",
  "author": "Plasticine-Yang",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development && nest start --watch",
    "dev:start": "docker compose -f docker-compose.dev.yml -p plasticine-monitor-server-dev start",
    "dev:stop": "docker compose -f docker-compose.dev.yml -p plasticine-monitor-server-dev stop",
    "dev:up": "docker compose -f docker-compose.dev.yml -p plasticine-monitor-server-dev up",
    "dev:down": "docker compose -f docker-compose.dev.yml -p plasticine-monitor-server-dev down",
    "build:dist": "cross-env NODE_ENV=production nest build",
    "build:image": "docker compose -f docker-compose.prod.yml -p plasticine-monitor-server build",
    "deploy": "pnpm build:dist && pnpm build:image && docker compose -f docker-compose.prod.yml -p plasticine-monitor-server up -d",
    "deploy:start": "docker compose -f docker-compose.prod.yml -p plasticine-monitor-server start",
    "deploy:stop": "docker compose -f docker-compose.prod.yml -p plasticine-monitor-server stop",
    "deploy:up": "docker compose -f docker-compose.prod.yml -p plasticine-monitor-server up",
    "deploy:down": "docker compose -f docker-compose.prod.yml -p plasticine-monitor-server down",
    "nest": "nest",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier -w",
    "format:src": "prettier -w src",
    "test": "vitest",
    "test:u": "vitest -u"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/mapped-types": "^1.2.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "0.13.0",
    "error-stack-parser": "^2.1.4",
    "joi": "^17.7.0",
    "mysql2": "2.2.5",
    "nestjs-pino": "^3.1.2",
    "pino-http": "^8.3.1",
    "pino-pretty": "^9.1.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "sonic-boom": "^3.2.1",
    "typeorm": "^0.3.11",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.15",
    "@types/node": "18.11.18",
    "@types/supertest": "^2.0.11",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4",
    "vitest": "^0.28.1",
    "webpack": "^5.75.0"
  }
}
